{
  "name": "slavasamsonow/telegram-bot-api",
  "description": "PHP Telegram Bot API library",
  "keywords": [
    "php",
    "telegram",
    "bot",
    "api",
    "telegram bot api"
  ],
  "homepage": "https://github/slavasamsonow/telegram-bot-api",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Slavasamsonow\\TelegramBotApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slavasamsonow\\TelegramBotApi\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Slava Samsonov",
      "email": "slavasamsonow@yandex.ru"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.11",
    "symfony/var-dumper": "^6.4"
  }
}
